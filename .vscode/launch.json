{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Extension Client",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceRoot}",
				"--disable-extensions"
			],
			"outFiles": [
				"${workspaceRoot}/dist/**/*.js"
			],
			"preLaunchTask": {
				"type": "npm",
				"script": "compile"
			}
		},
	]
}
